{
    "name": "love-launcher",
    "displayName": "love-launcher",
    "description": "Launches your project in Löve",
    "version": "0.0.1",
    "publisher": "JanW",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.launch"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.launch",
            "title": "Launch Löve"
        }],
        "keybindings":[
            {
                "command": "extension.launch",
                "key": "Alt+L"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}